{
  "compilerOptions": {
    //maybe switch back to es6 or es2020
    "target": "es6",
    "module": "CommonJS",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "**/*.ts",
    "express.d.ts",
    "middleware/authentication.ts",
    "config.ts"
  ],
  "exclude": ["node_modules", "dist"]
}
